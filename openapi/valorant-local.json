    "/entitlements/v1/token": {
      "get": {
        "description": "Gets both the token and entitlement for API usage\n`accessToken` is used as the token and `token` is used as the entitlement.",
        "summary": "Entitlements Token",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "required": true,
            "name": "localAuth",
            "in": "header"
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "accessToken": {
                      "type": "string",
                      "description": "Used as the token in requests"
                    },
                    "entitlements": {
                      "type": "array",
                      "items": {
                        "nullable": true
                      }
                    },
                    "issuer": {
                      "type": "string"
                    },
                    "subject": {
                      "type": "string",
                      "pattern": "^([a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12})$",
                      "description": "Player UUID"
                    },
                    "token": {
                      "type": "string",
                      "description": "Used as the entitlement in requests"
                    }
                  },
                  "required": [
                    "accessToken",
                    "entitlements",
                    "issuer",
                    "subject",
                    "token"
                  ]
                }
              }
            }
          }
        }
      }
    },
