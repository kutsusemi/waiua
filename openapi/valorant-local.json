  "paths": {
    "/help": {
      "get": {
        "description": "Get help for the local client",
        "summary": "Local Help",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "required": true,
            "name": "localAuth",
            "in": "header"
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "events": {
                      "type": "object",
                      "additionalProperties": {
                        "type": "string",
                        "description": "Websocket event description"
                      }
                    },
                    "functions": {
                      "type": "object",
                      "additionalProperties": {
                        "type": "string",
                        "description": "Function description"
                      }
                    },
                    "types": {
                      "type": "object",
                      "additionalProperties": {
                        "type": "string",
                        "description": "Type description"
                      }
                    }
                  },
                  "required": [
                    "events",
                    "functions",
                    "types"
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/product_session/v1/external_sessions": {
      "get": {
        "description": "Gets info about the running Valorant process including start arguments\n  Can be used to get shard, region, and puuid by parsing launch args.",
        "summary": "Sessions",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "required": true,
            "name": "localAuth",
            "in": "header"
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "object",
                    "properties": {
                      "exitCode": {
                        "type": "number"
                      },
                      "exitReason": {
                        "nullable": true
                      },
                      "isInternal": {
                        "type": "boolean"
                      },
                      "launchConfiguration": {
                        "type": "object",
                        "properties": {
                          "arguments": {
                            "type": "array",
                            "items": {
                              "type": "string"
                            }
                          },
                          "executable": {
                            "type": "string"
                          },
                          "locale": {
                            "type": "string",
                            "nullable": true
                          },
                          "voiceLocale": {
                            "nullable": true
                          },
                          "workingDirectory": {
                            "type": "string"
                          }
                        },
                        "required": [
                          "arguments",
                          "executable",
                          "locale",
                          "voiceLocale",
                          "workingDirectory"
                        ]
                      },
                      "patchlineFullName": {
                        "type": "string",
                        "enum": [
                          "VALORANT",
                          "riot_client"
                        ]
                      },
                      "patchlineId": {
                        "type": "string",
                        "enum": [
                          "",
                          "live",
                          "pbe"
                        ]
                      },
                      "phase": {
                        "type": "string"
                      },
                      "productId": {
                        "type": "string",
                        "enum": [
                          "valorant",
                          "riot_client"
                        ]
                      },
                      "version": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "exitCode",
                      "exitReason",
                      "isInternal",
                      "launchConfiguration",
                      "patchlineFullName",
                      "patchlineId",
                      "phase",
                      "productId",
                      "version"
                    ]
                  }
                }
              }
            }
          }
        }
      }
    },
